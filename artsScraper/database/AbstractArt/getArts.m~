expt_dir = {'/Users/sandy/Dropbox/Caltech/AbArts/artsScraper/database/AbstractArt'};
arts_wildcard = '*.jpg';

arts_dirs = {};
for d = expt_dir
    fullarts = dir(fullfile(d{1}, arts_wildcard));
    for i = 1 : numel(fullarts)
        arts_dirs = [arts_dirs ...
            fullfile(d{1}, fullarts(i).name)];
    end;
end;

nArts = size(arts_dirs,2);

randomArts = randperm(nArts);

randomArts(1:200)

